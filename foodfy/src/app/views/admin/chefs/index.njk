{% extends "../layout.njk" %}

{% block content %}
<h3>Gerenciar chefs</h3>

<div class="chefs">
{% for chef in chefs %}
<div class="card_chefs">
    <img src="{{chef.avatar_url}}" alt="Imagem do/da chefe {{chef.name}}">
    <div>
        <p>{{chef.name}}</p>
        <a href="/admin/chefs/{{chef.id}}">Visualizar</a>
    </div>
</div>
{% endfor %}
</div>

{% endblock content %}

